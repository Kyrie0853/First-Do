#include <stdio.h>

int main() {
    char a;
    char b = 'Y'; 
    
    printf("2023.10.26 is your first time creat the github\n");
    printf("如果你要继续，请说YES，放弃请说NO\n");
    
    scanf(" %c", &a); 
    
    if (a == b) {
        printf("加油！\n");
    } else {
        printf("滚！\n");
    }
    
    return 0;
}
